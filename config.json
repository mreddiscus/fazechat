{
  "socket.io": {
    "enabled": true,
    "host": "carbon"
  },
  
  "airborne": {
    "enabled": true,
    "url": "http://baconfarm.org:8180"
  },

  "rest": {
    "autoGenerateProxy": true
  },
  
  "auth":{
    "enabled": true,
    "userIdPrefix": "",
    "rules": {
      "^/$": {
        "loginRequired": true,
        "redirectUrl": "/login"
      }
    }
  },
  
  "logging": {
    "categories": {
      "http.access": "off"
    }
  },
  
  "ui": {
    "provider": "bootstrap"
  }
}
